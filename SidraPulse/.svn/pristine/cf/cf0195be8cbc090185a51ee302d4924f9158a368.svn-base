package com.atomix.datamodel;

public class OfferPromotionPhoto {
	private int id;
	private int offer_and_promotion_id;
	private String photo;
	private int isDelete;
	
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public int getOffer_and_promotion_id() {
		return offer_and_promotion_id;
	}
	public void setOffer_and_promotion_id(int offer_and_promotion_id) {
		this.offer_and_promotion_id = offer_and_promotion_id;
	}
	public String getPhoto() {
		return photo;
	}
	public void setPhoto(String photo) {
		this.photo = photo;
	}
	public int getIsDelete() {
		return isDelete;
	}
	public void setIsDelete(int isDelete) {
		this.isDelete = isDelete;
	}

}
